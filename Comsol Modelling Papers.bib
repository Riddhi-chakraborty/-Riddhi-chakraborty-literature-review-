  langid = {english},
  keywords = {ionomer,PEFC,PEM,PEMFC,polymer electrolyte membrane,polymer electrolyte membrane fuel cell,proton exchange membrane,proton exchange membrane fuel cell},
  file = {C:\Users\chakrabo\Zotero\storage\Z85JQXZ9\Dickinson and Smith - 2020 - Modelling the Proton-Conductive Membrane in Practical Polymer Electrolyte Membrane Fuel Cell (PEMFC).pdf}
